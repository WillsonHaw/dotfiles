{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022"],
    "outDir": "../.config",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "declaration": false,
    "noImplicitAny": false,
    "baseUrl": ".",
